package ds.stack;

public class App {

	public static void main(String[] args) {
		Stack theStack = new Stack(10);
		theStack.push('h');
		theStack.push('e');
		theStack.push('l');
		theStack.push('l');
		theStack.push('o');
		
		while(!theStack.isEmpty()) {
			long value = theStack.pop();
			System.out.println(value);
		}
		
		
		
	}

}
