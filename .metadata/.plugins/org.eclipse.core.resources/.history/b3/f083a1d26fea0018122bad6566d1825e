package placement.assignment.session1;

public class DisplayNumbers {
	
	public void displayNumbers(){
	for(int i = 1; i<=100; i++) {
		System.out.println(i);
	}
	}

	public static void main(String[] args) {
		displayNumbers oneToHundred = new displayNumbers();
		oneToHundred.displayNumbers();

	}

}
