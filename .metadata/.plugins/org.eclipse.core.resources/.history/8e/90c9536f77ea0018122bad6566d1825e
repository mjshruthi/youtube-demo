package placement.assignment.session1;

public class Factorial {
	
	static int findFactorial(int n) {
		
		int res = 1, i;
		for(i = 2; i<=n; i++)
			res *= i;
			return res;
		//System.out.println("Factorial of "+x +" is"+ i);
	}

	public static void main(String[] args) {
		int num = 5;
	    findFactorial(3);

	}

}
